    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "apollo-server-express": "^2.3.1",
    "dotenv-safe": "^6.1.0",
    "express": "^4.16.4",
    "graphql": "^14.0.2",
    "pg": "^7.7.1",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.11"
  }
}
